@startuml
!define table(name) entity name << (T,#5DBCD2) >>
!define view(name) entity name << (V,#C6EDDB) >>
hide methods
hide stereotypes

skinparam class {
  BackgroundColor White
  BorderColor #6E6E6E
  ArrowColor #6E6E6E
}

' tables
table("a") {
  a 
  a2 
}
table("b") {
  b 
  b2 
}

' relations
"a" -- "b" : ""

@enduml
