@startuml
!define table(name) entity name << (T,#5DBCD2) >>
!define view(name) entity name << (V,#C6EDDB) >>
hide methods
hide stereotypes

skinparam class {
  BackgroundColor White
  BorderColor #6E6E6E
  ArrowColor #6E6E6E
}

' tables
table("a") {
  + a <font color="#666666">[]</font>
  a2 <font color="#666666">[]</font>
}
table("b") {
  # b <font color="#666666">[]</font>
  b2 <font color="#666666">[]</font>
}

' relations
"b" }-- "a" : ""

@enduml
